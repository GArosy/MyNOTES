/* pages/me/me.wxss */
.userinfo {
  width: 100%;
  height: auto;
  margin-bottom: 20rpx;
  background-color: white;
  display: flex;
  flex-direction: row;
  align-items: flex-start;
}
.userinfo-avatar {
  overflow: hidden;
  width: 128rpx;
  height: 128rpx;
  margin: 50rpx;
  border-radius: 10rpx;
}
.userinfo-name {
  font-size: 40rpx;
  font-weight: 450 ;
  margin-top: 50rpx;
}
.me-support{
  width: 100%;
  height: auto;
  background-color: white;
  display: flex;
  flex-direction: column;
  align-content: center;
  justify-content: flex-start;
}
.text {
  padding: 30rpx 0rpx 30rpx 50rpx;
}
.textAct {
  padding: 30rpx 0rpx 30rpx 50rpx;
  color: gray;
}
.contact {
  height: auto;
  background-color: white;
  font-weight: normal;
}
.divLine {
  width: 100%;
  height: 1rpx;
  background-color: rgb(230, 230, 230);
  z-index: 1;
}